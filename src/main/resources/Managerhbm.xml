<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="fr.givemeacar.app.model.Manager" table="manager" schema="givemeacar">
        <id name="id">
            <column name="id" sql-type="int"/>
        </id>
        <property name="firstName">
            <column name="firstName" sql-type="varchar(32)" length="32"/>
        </property>
        <property name="lastName">
            <column name="lastName" sql-type="varchar(32)" length="32"/>
        </property>
        <property name="mail">
            <column name="mail" sql-type="varchar(64)" length="64"/>
        </property>
        <property name="password">
            <column name="password" sql-type="varchar(64)" length="64"/>
        </property>
        <property name="phone">
            <column name="phone" sql-type="varchar(12)" length="12"/>
        </property>
        <property name="userStatusId">
            <column name="user_status_id" sql-type="int"/>
        </property>
        <property name="agencyId">
            <column name="agency_id" sql-type="int"/>
        </property>
        <many-to-one name="userStatusByUserStatusId" class="fr.givemeacar.app.model.UserStatus">
            <column name="user_status_id"/>
        </many-to-one>
        <many-to-one name="agencyByAgencyId" class="fr.givemeacar.app.model.Agency">
            <column name="agency_id"/>
        </many-to-one>
        <set name="agenciesById" inverse="true">
            <key>
                <column name="manager_id"/>
            </key>
            <one-to-many not-found="ignore" class="fr.givemeacar.app.model.Agency"/>
        </set>
    </class>
</hibernate-mapping>