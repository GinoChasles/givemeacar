<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="fr.givemeacar.app.model.Address" table="address" schema="givemeacar2">
        <id name="id">
            <column name="id" sql-type="int"/>
        </id>
        <property name="number">
            <column name="number" sql-type="int" not-null="true"/>
        </property>
        <property name="localities">
            <column name="localities" sql-type="varchar(64)" length="64" not-null="true"/>
        </property>
        <property name="streetId">
            <column name="street_id" sql-type="int"/>
        </property>
        <property name="cityId">
            <column name="city_id" sql-type="int"/>
        </property>
        <property name="streetSuffixId">
            <column name="street_suffix_id" sql-type="int"/>
        </property>
    </class>
</hibernate-mapping>