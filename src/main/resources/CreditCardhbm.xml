<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="fr.givemeacar.app.model.CreditCard" table="credit_card" schema="givemeacar2">
        <id name="id">
            <column name="id" sql-type="int"/>
        </id>
        <property name="number">
            <column name="number" sql-type="varchar(16)" length="16"/>
        </property>
        <property name="expirationMonth">
            <column name="expirationMonth" sql-type="int"/>
        </property>
        <property name="expirationYear">
            <column name="expirationYear" sql-type="int"/>
        </property>
        <property name="first_name">
            <column name="first_name" sql-type="varchar(45)" length="45"/>
        </property>
        <property name="last_name">
            <column name="last_name" sql-type="varchar(45)" length="45"/>
        </property>
        <property name="society">
            <column name="society" sql-type="varchar(45)" length="45" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>