<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="fr.givemeacar.app.model.Rent" table="rent" schema="givemeacar2">
        <id name="id">
            <column name="id" sql-type="int"/>
        </id>
        <property name="start">
            <column name="start" sql-type="datetime"/>
        </property>
        <property name="end">
            <column name="end" sql-type="datetime" not-null="true"/>
        </property>
        <property name="carId">
            <column name="car_id" sql-type="int"/>
        </property>
        <property name="clientId">
            <column name="client_id" sql-type="int"/>
        </property>
    </class>
</hibernate-mapping>