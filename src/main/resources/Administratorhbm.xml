<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="fr.givemeacar.app.model.Administrator" table="administrator" schema="givemeacar2">
        <id name="id">
            <column name="id" sql-type="int"/>
        </id>
        <property name="first_name">
            <column name="first_name" sql-type="varchar(32)" length="32"/>
        </property>
        <property name="last_name">
            <column name="last_name" sql-type="varchar(32)" length="32"/>
        </property>
        <property name="mail">
            <column name="mail" sql-type="varchar(64)" length="64"/>
        </property>
        <property name="password">
            <column name="password" sql-type="varchar(45)" length="45"/>
        </property>
        <property name="phone">
            <column name="phone" sql-type="varchar(12)" length="12"/>
        </property>
        <property name="userStatusId">
            <column name="user_status_id" sql-type="int"/>
        </property>
    </class>
</hibernate-mapping>