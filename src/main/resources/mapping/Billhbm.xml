<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="fr.givemeacar.app.model.Bill" table="bill" schema="givemeacar">
        <id name="id">
            <column name="id" sql-type="int"/>
        </id>
        <property name="price">
            <column name="price" sql-type="double" precision="-1"/>
        </property>
        <property name="date">
            <column name="date" sql-type="date"/>
        </property>
        <property name="rentId">
            <column name="rent_id" sql-type="int"/>
        </property>
        <property name="agencyId">
            <column name="agency_id" sql-type="int"/>
        </property>
        <many-to-one name="rentByRentId" class="fr.givemeacar.app.model.Rent">
            <column name="rent_id"/>
        </many-to-one>
        <many-to-one name="agencyByAgencyId" class="fr.givemeacar.app.model.Agency">
            <column name="agency_id"/>
        </many-to-one>
        <set name="agenciesById" inverse="true">
            <key>
                <column name="bill_id"/>
            </key>
            <one-to-many not-found="ignore" class="fr.givemeacar.app.model.Agency"/>
        </set>
        <set name="clientsById" inverse="true">
            <key>
                <column name="bill_id"/>
            </key>
            <one-to-many not-found="ignore" class="fr.givemeacar.app.model.Client"/>
        </set>
    </class>
</hibernate-mapping>