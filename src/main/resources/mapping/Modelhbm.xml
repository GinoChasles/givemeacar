<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="fr.givemeacar.app.model.Model" table="model" schema="givemeacar">
        <id name="id">
            <column name="id" sql-type="int"/>
        </id>
        <property name="name">
            <column name="name" sql-type="varchar(45)" length="45"/>
        </property>
        <property name="brandId">
            <column name="brand_id" sql-type="int"/>
        </property>
        <many-to-one name="brandByBrandId" class="fr.givemeacar.app.model.Brand">
            <column name="brand_id"/>
        </many-to-one>
        <set name="brandsById" inverse="true">
            <key>
                <column name="model_id"/>
            </key>
            <one-to-many not-found="ignore" class="fr.givemeacar.app.model.Brand"/>
        </set>
        <set name="carsById" inverse="true">
            <key>
                <column name="model_id"/>
            </key>
            <one-to-many not-found="ignore" class="fr.givemeacar.app.model.Car"/>
        </set>
    </class>
</hibernate-mapping>