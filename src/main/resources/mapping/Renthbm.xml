<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="fr.givemeacar.app.model.Rent" table="rent" schema="givemeacar">
        <id name="id">
            <column name="id" sql-type="int"/>
        </id>
        <property name="start">
            <column name="start" sql-type="datetime"/>
        </property>
        <property name="end">
            <column name="end" sql-type="datetime" not-null="true"/>
        </property>
        <property name="carId">
            <column name="car_id" sql-type="int"/>
        </property>
        <property name="clientId">
            <column name="client_id" sql-type="int"/>
        </property>
        <many-to-one name="carByCarId" class="fr.givemeacar.app.model.Car">
            <column name="car_id"/>
        </many-to-one>
        <many-to-one name="clientByClientId" class="fr.givemeacar.app.model.Client">
            <column name="client_id"/>
        </many-to-one>
        <set name="billsById" inverse="true">
            <key>
                <column name="rent_id"/>
            </key>
            <one-to-many not-found="ignore" class="fr.givemeacar.app.model.Bill"/>
        </set>
        <set name="carsById" inverse="true">
            <key>
                <column name="rent_id"/>
            </key>
            <one-to-many not-found="ignore" class="fr.givemeacar.app.model.Car"/>
        </set>
    </class>
</hibernate-mapping>