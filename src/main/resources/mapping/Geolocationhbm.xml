<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="fr.givemeacar.app.model.Geolocation" table="geolocation" schema="givemeacar">
        <id name="id">
            <column name="id" sql-type="int"/>
        </id>
        <property name="latitude">
            <column name="latitude" sql-type="double" precision="-1"/>
        </property>
        <property name="longitude">
            <column name="longitude" sql-type="double" precision="-1"/>
        </property>
        <set name="carsById" inverse="true">
            <key>
                <column name="geolocation_id1"/>
            </key>
            <one-to-many not-found="ignore" class="fr.givemeacar.app.model.Car"/>
        </set>
    </class>
</hibernate-mapping>