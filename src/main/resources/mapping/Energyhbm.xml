<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="fr.givemeacar.app.model.Energy" table="energy" schema="givemeacar">
        <id name="id">
            <column name="id" sql-type="int"/>
        </id>
        <property name="max">
            <column name="max" sql-type="tinyint"/>
        </property>
        <property name="current">
            <column name="current" sql-type="tinyint"/>
        </property>
        <property name="energyTypeId">
            <column name="energy_type_id" sql-type="int"/>
        </property>
        <many-to-one name="energyTypeByEnergyTypeId" class="fr.givemeacar.app.model.EnergyType">
            <column name="energy_type_id"/>
        </many-to-one>
        <set name="carsById" inverse="true">
            <key>
                <column name="energy_id"/>
            </key>
            <one-to-many not-found="ignore" class="fr.givemeacar.app.model.Car"/>
        </set>
    </class>
</hibernate-mapping>