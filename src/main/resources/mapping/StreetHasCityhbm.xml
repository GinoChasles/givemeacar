<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="fr.givemeacar.app.model.StreetHasCity" table="street_has_city" schema="givemeacar">
        <composite-id mapped="true" class="fr.givemeacar.app.model.StreetHasCityPK">
            <key-property name="streetId">
                <column name="street_id" sql-type="int"/>
            </key-property>
            <key-property name="cityId">
                <column name="city_id" sql-type="int"/>
            </key-property>
        </composite-id>
        <many-to-one name="streetByStreetId" class="fr.givemeacar.app.model.Street">
            <column name="street_id"/>
        </many-to-one>
        <many-to-one name="cityByCityId" class="fr.givemeacar.app.model.City">
            <column name="city_id"/>
        </many-to-one>
    </class>
</hibernate-mapping>