<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="fr.givemeacar.app.model.Address" table="address" schema="givemeacar">
        <id name="id">
            <column name="id" sql-type="int"/>
        </id>
        <property name="number">
            <column name="number" sql-type="int" not-null="true"/>
        </property>
        <property name="localities">
            <column name="localities" sql-type="varchar(64)" length="64" not-null="true"/>
        </property>
        <property name="streetId">
            <column name="street_id" sql-type="int"/>
        </property>
        <property name="cityId">
            <column name="city_id" sql-type="int"/>
        </property>
        <property name="streetSuffixId">
            <column name="street_suffix_id" sql-type="int"/>
        </property>
        <many-to-one name="streetByStreetId" class="fr.givemeacar.app.model.Street">
            <column name="street_id"/>
        </many-to-one>
        <many-to-one name="cityByCityId" class="fr.givemeacar.app.model.City">
            <column name="city_id"/>
        </many-to-one>
        <many-to-one name="streetSuffixByStreetSuffixId" class="fr.givemeacar.app.model.StreetSuffix">
            <column name="street_suffix_id"/>
        </many-to-one>
        <set name="agenciesById" inverse="true">
            <key>
                <column name="address_id"/>
            </key>
            <one-to-many not-found="ignore" class="fr.givemeacar.app.model.Agency"/>
        </set>
        <set name="clientsById" inverse="true">
            <key>
                <column name="address_id"/>
            </key>
            <one-to-many not-found="ignore" class="fr.givemeacar.app.model.Client"/>
        </set>
    </class>
</hibernate-mapping>