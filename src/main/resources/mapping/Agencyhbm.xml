<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="fr.givemeacar.app.model.Agency" table="agency" schema="givemeacar">
        <id name="id">
            <column name="id" sql-type="int"/>
        </id>
        <property name="name">
            <column name="name" sql-type="varchar(32)" length="32"/>
        </property>
        <property name="managerId">
            <column name="manager_id" sql-type="int"/>
        </property>
        <property name="clientId1">
            <column name="client_id1" sql-type="int"/>
        </property>
        <property name="addressId">
            <column name="address_id" sql-type="int"/>
        </property>
        <property name="billId">
            <column name="bill_id" sql-type="int"/>
        </property>
        <many-to-one name="managerByManagerId" class="fr.givemeacar.app.model.Manager">
            <column name="manager_id"/>
        </many-to-one>
        <many-to-one name="clientByClientId1" class="fr.givemeacar.app.model.Client">
            <column name="client_id1"/>
        </many-to-one>
        <many-to-one name="addressByAddressId" class="fr.givemeacar.app.model.Address">
            <column name="address_id"/>
        </many-to-one>
        <many-to-one name="billByBillId" class="fr.givemeacar.app.model.Bill">
            <column name="bill_id"/>
        </many-to-one>
        <set name="billsById" inverse="true">
            <key>
                <column name="agency_id"/>
            </key>
            <one-to-many not-found="ignore" class="fr.givemeacar.app.model.Bill"/>
        </set>
        <set name="clientsById" inverse="true">
            <key>
                <column name="agency_id"/>
            </key>
            <one-to-many not-found="ignore" class="fr.givemeacar.app.model.Client"/>
        </set>
        <set name="managersById" inverse="true">
            <key>
                <column name="agency_id"/>
            </key>
            <one-to-many not-found="ignore" class="fr.givemeacar.app.model.Manager"/>
        </set>
    </class>
</hibernate-mapping>