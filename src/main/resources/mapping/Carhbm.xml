<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="fr.givemeacar.app.model.Car" table="car" schema="givemeacar">
        <id name="id">
            <column name="id" sql-type="int"/>
        </id>
        <property name="kilometers">
            <column name="kilometers" sql-type="int"/>
        </property>
        <property name="price">
            <column name="price" sql-type="double" precision="-1"/>
        </property>
        <property name="year">
            <column name="year" sql-type="int"/>
        </property>
        <property name="availability">
            <column name="availability" sql-type="tinyint unsigned"/>
        </property>
        <property name="onRent">
            <column name="onRent" sql-type="tinyint unsigned"/>
        </property>
        <property name="colorId1">
            <column name="color_id1" sql-type="int"/>
        </property>
        <property name="geolocationId1">
            <column name="geolocation_id1" sql-type="int"/>
        </property>
        <property name="modelId">
            <column name="model_id" sql-type="int"/>
        </property>
        <property name="energyId">
            <column name="energy_id" sql-type="int"/>
        </property>
        <property name="rentId">
            <column name="rent_id" sql-type="int"/>
        </property>
        <many-to-one name="colorByColorId1" class="fr.givemeacar.app.model.Color">
            <column name="color_id1"/>
        </many-to-one>
        <many-to-one name="geolocationByGeolocationId1" class="fr.givemeacar.app.model.Geolocation">
            <column name="geolocation_id1"/>
        </many-to-one>
        <many-to-one name="modelByModelId" class="fr.givemeacar.app.model.Model">
            <column name="model_id"/>
        </many-to-one>
        <many-to-one name="energyByEnergyId" class="fr.givemeacar.app.model.Energy">
            <column name="energy_id"/>
        </many-to-one>
        <many-to-one name="rentByRentId" class="fr.givemeacar.app.model.Rent">
            <column name="rent_id"/>
        </many-to-one>
        <set name="rentsById" inverse="true">
            <key>
                <column name="car_id"/>
            </key>
            <one-to-many not-found="ignore" class="fr.givemeacar.app.model.Rent"/>
        </set>
    </class>
</hibernate-mapping>