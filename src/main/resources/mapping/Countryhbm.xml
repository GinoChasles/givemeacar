<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="fr.givemeacar.app.model.Country" table="country" schema="givemeacar">
        <id name="id">
            <column name="id" sql-type="int"/>
        </id>
        <property name="name">
            <column name="name" sql-type="varchar(42)" length="42"/>
        </property>
        <set name="citiesById" inverse="true">
            <key>
                <column name="country_id"/>
            </key>
            <one-to-many not-found="ignore" class="fr.givemeacar.app.model.City"/>
        </set>
    </class>
</hibernate-mapping>