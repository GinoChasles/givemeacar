<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="fr.givemeacar.app.model.Bill" table="bill" schema="givemeacar2">
        <id name="id">
            <column name="id" sql-type="int"/>
        </id>
        <property name="price">
            <column name="price" sql-type="double" precision="-1"/>
        </property>
        <property name="date">
            <column name="date" sql-type="date"/>
        </property>
        <property name="rentId">
            <column name="rent_id" sql-type="int"/>
        </property>
        <property name="agencyId">
            <column name="agency_id" sql-type="int"/>
        </property>
    </class>
</hibernate-mapping>