<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="fr.givemeacar.app.model.City" table="city" schema="givemeacar2">
        <id name="id">
            <column name="id" sql-type="int"/>
        </id>
        <property name="name">
            <column name="name" sql-type="varchar(68)" length="68"/>
        </property>
        <property name="zipcode">
            <column name="zipcode" sql-type="varchar(5)" length="5"/>
        </property>
        <property name="countryId">
            <column name="country_id" sql-type="int"/>
        </property>
    </class>
</hibernate-mapping>