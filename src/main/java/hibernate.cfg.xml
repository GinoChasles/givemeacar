        <property name="connection.url">jdbc:mysql://localhost:3306/givemeacar?serverTimezone=UTC</property>
        <property name="connection.driver_class">com.mysql.jdbc.Driver</property>
        <mapping class="fr.givemeacar.app.model.Address"/>
        <mapping class="fr.givemeacar.app.model.Administrator"/>
        <mapping class="fr.givemeacar.app.model.Agency"/>
        <mapping class="fr.givemeacar.app.model.Bill"/>
        <mapping class="fr.givemeacar.app.model.Brand"/>
        <mapping class="fr.givemeacar.app.model.Car"/>
        <mapping class="fr.givemeacar.app.model.City"/>
        <mapping class="fr.givemeacar.app.model.Client"/>
        <mapping class="fr.givemeacar.app.model.Color"/>
        <mapping class="fr.givemeacar.app.model.Country"/>
        <mapping class="fr.givemeacar.app.model.CreditCard"/>
        <mapping class="fr.givemeacar.app.model.Department"/>
        <mapping class="fr.givemeacar.app.model.EnergyType"/>
        <mapping class="fr.givemeacar.app.model.Manager"/>
        <mapping class="fr.givemeacar.app.model.Model"/>
        <mapping class="fr.givemeacar.app.model.Region"/>
        <mapping class="fr.givemeacar.app.model.Rent"/>
        <mapping class="fr.givemeacar.app.model.Street"/>
        <mapping class="fr.givemeacar.app.model.StreetName"/>
        <mapping class="fr.givemeacar.app.model.StreetSuffix"/>
        <mapping class="fr.givemeacar.app.model.UserStatus"/>
        <!-- <property name="connection.username"/> -->
        <!-- <property name="connection.password"/> -->

        <!-- DB schema will be updated if needed -->
        <!-- <property name="hibernate.hbm2ddl.auto">update</property> -->
    </session-factory>
</hibernate-configuration>